{
  "name": "dunbar",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsnd --files --respawn --exit-child -r tsconfig-paths/register main.ts",
    "build": "NODE_OPTIONS='--max-old-space-size=8192' tsc --project tsconfig.json",
    "start": "node dist/main.js",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:reset": "npx prisma db push --force-reset && ts-node prisma/seed.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "run:example": "yarn build && node dist/scripts/example.js",
    "run:faucet": "yarn build && node dist/scripts/faucet.js"
  },
